class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        arr=set(nums)
        seq_c=0
        for i in arr:
            if i-1 not in arr:
                c=0
                while i+c in arr:
                    c=c+1
                seq_c=max(seq_c,c)
        return seq_c
